<Window x:Class="OpenLearnAnswerFetcher.Client.EnteranceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OpenLearnAnswerFetcher.Client"
        mc:Ignorable="d"
        Title="EnteranceWindow" Height="300" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="ButtonFetcher" Content="Fetch" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Center" Width="75" Click="ButtonFetcher_Click"/>
        <Button x:Name="ButtonSimulator" Content="Simulate" Grid.Column="1" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Center" Width="75" Click="ButtonSimulator_Click"/>

    </Grid>
</Window>
